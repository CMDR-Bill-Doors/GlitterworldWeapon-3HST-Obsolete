<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--=====================Disposable LGIS Launchers ===================-->
	<ThingDef Name="BaseDisposableLGIS" Abstract="True">
		<description>Minature missile with low-end laser guidance, designed to be carried by infantry or light armored vehicles, capable of beam-riding guidance, inertia guidance or unguided dumb fire. The missile comes in protective packaging that can also be used as a disposable launcher.</description>
		<soundInteract>Interact_Rifle</soundInteract>
		<category>Item</category>
		<thingClass>ThingWithComps</thingClass>
		<equipmentType>Primary</equipmentType>
		<pathCost>10</pathCost>
		<useHitPoints>True</useHitPoints>
		<selectable>True</selectable>
		<stackLimit>50</stackLimit>
		<drawGUIOverlay>true</drawGUIOverlay>
		<storedConceptLearnOpportunity>BDGW_LGIS</storedConceptLearnOpportunity>

		<graphicData>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>

		<statBases>
			<SightsEfficiency>1.0</SightsEfficiency>
			<ShotSpread>0.1</ShotSpread>
			<SwayFactor>1.2</SwayFactor>
			<Bulk>6.8</Bulk>
			<Mass>2.5</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<SellPriceFactor>0.5</SellPriceFactor>
			<RangedWeapon_Cooldown>2.85</RangedWeapon_Cooldown>
		</statBases>

		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>True</alwaysHaulable>
		<tickerType>Normal</tickerType>
		<techLevel>Spacer</techLevel>

		<thingCategories>
			<li>Gun3HSTDisposable</li>
		</thingCategories>
		<thingSetMakerTags>
			<li>SingleUseWeapon</li>
			<li>RewardStandardMidFreq</li>
		</thingSetMakerTags>
		<weaponClasses>
			<li>RangedHeavy</li>
		</weaponClasses>

		<weaponTags>
			<li>CE_AI_AOE</li>
			<li>3HST_Weapons</li>
		</weaponTags>

		<generateCommonality>2</generateCommonality>
		<comps>
			<li Class="CompProperties_Forbiddable" />
			<li>
				<compClass>CompEquippable</compClass>
			</li>
			<li Class="CombatExtended.CompProperties_FireModes">
				<aiUseBurstMode>FALSE</aiUseBurstMode>
				<aiAimMode>AimedShot</aiAimMode>
			</li>
			<li Class="ThreeHST.CompProperties_ShowConcept">
				<concepts>
					<li>BDGW_LGIS</li>
				</concepts>
			</li>
		</comps>

		<tools>
			<li Class="CombatExtended.ToolCE">
				<label>stock</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>8</power>
				<cooldownTime>1.55</cooldownTime>
				<chanceFactor>1.5</chanceFactor>
				<armorPenetrationBlunt>2.755</armorPenetrationBlunt>
				<linkedBodyPartsGroup>Stock</linkedBodyPartsGroup>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>barrel</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2.02</cooldownTime>
				<armorPenetrationBlunt>1.630</armorPenetrationBlunt>
				<linkedBodyPartsGroup>Barrel</linkedBodyPartsGroup>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>muzzle</label>
				<capacities>
					<li>Poke</li>
				</capacities>
				<power>8</power>
				<cooldownTime>1.55</cooldownTime>
				<armorPenetrationBlunt>2.755</armorPenetrationBlunt>
				<linkedBodyPartsGroup>Muzzle</linkedBodyPartsGroup>
			</li>
		</tools>

		<modExtensions>
			<li Class="CombatExtended.GunDrawExtension">
				<DrawSize>1.0,1.0</DrawSize>
				<DrawOffset>-0.3,0.0</DrawOffset>
			</li>
		</modExtensions>
	</ThingDef>

	<ThingDef Class="CombatExtended.AmmoDef" Name="BaseDisposableLGISammo" ParentName="BaseDisposableLGIS" Abstract="True">
		<description>Minature missile with low-end laser guidance, designed to be carried by infantry or light armored vehicles, capable of beam-riding guidance, inertia guidance or unguided dumb fire. The missile comes in protective packaging that can also be used as a disposable launcher, but it is also used to feed various LGIS launchers.</description>

		<thingClass>CombatExtended.AmmoThing</thingClass>
		<cookOffFlashScale>40</cookOffFlashScale>
		<cookOffSound>MortarBomb_Explode</cookOffSound>

		<thingCategories>
			<li>AmmoCategory70mmLGIS</li>
		</thingCategories>

		<weaponTags>
			<li>CE_AI_Rifle</li>
			<li>CE_AI_Launcher</li>
			<li>3HST_Weapons</li>
		</weaponTags>
		<tradeTags>
			<li>CE_HeavyAmmo</li>
		</tradeTags>
	</ThingDef>

	<!--===================Direct launchers/ammo=============-->
	<ThingDef Class="CombatExtended.AmmoDef" ParentName="BaseDisposableLGISammo">
		<defName>Gun_3HST_LGIS_frag</defName>
		<label>frag LGIS tube</label>
		<ammoClass>RocketFrag</ammoClass>
		<detonateProjectile>Projectile_70mmLGIS_Frag</detonateProjectile>

		<graphicData>
			<texPath>Things/Weapons/Disposable_LGIS/frag</texPath>
		</graphicData>
		<!--<uiIconPath>UI/Icons/Rockets/3HST_IAA_12_LGIS_Missile_frag_a</uiIconPath>-->

		<statBases>
			<MarketValue>107</MarketValue>
		</statBases>
		<weaponTags>
			<li>GunHeavy</li>
			<li>IndustrialGunAdvanced</li>
			<li>3HST_LGIS_Disposable</li>
			<li>Gun</li>
			<li>GunSingleUse</li>
		</weaponTags>
		<verbs>
			<li Class="CombatExtended.VerbPropertiesCE">
				<verbClass>ThreeHST.Verb_ShootCEAutoSwitchModeOneUse</verbClass>
				<defaultProjectile>Projectile_70mmLGIS_Frag</defaultProjectile>
				<hasStandardCommand>true</hasStandardCommand>
				<ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
				<warmupTime>3.4</warmupTime>
				<burstShotCount />
				<range>60</range>
				<minRange>10</minRange>
				<targetParams>
					<canTargetLocations>true</canTargetLocations>
				</targetParams>
				<soundCast>InfernoCannon_Fire</soundCast>
				<soundCastTail>GunTail_Medium</soundCastTail>
				<muzzleFlashScale>5</muzzleFlashScale>
				<ai_AvoidFriendlyFireRadius>5</ai_AvoidFriendlyFireRadius>
				<circularError>10</circularError>
				<requireLineOfSight>false</requireLineOfSight>
			</li>
		</verbs>
		<comps>
			<li Class="ThreeHST.CompProperties_SecondaryVerb">
				<verbProps>
					<verbClass>ThreeHST.Verb_ShootCEAutoSwitchModeOneUse</verbClass>
					<defaultProjectile>Projectile_70mmLGIS_Frag_Indirect</defaultProjectile>
					<hasStandardCommand>true</hasStandardCommand>
					<ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
					<warmupTime>6.4</warmupTime>
					<range>90</range>
					<minRange>20</minRange>
					<targetParams>
						<canTargetLocations>true</canTargetLocations>
					</targetParams>
					<soundCast>InfernoCannon_Fire</soundCast>
					<soundCastTail>GunTail_Medium</soundCastTail>
					<muzzleFlashScale>5</muzzleFlashScale>
					<ignorePartialLoSBlocker>true</ignorePartialLoSBlocker>
					<circularError>3</circularError>
					<ai_AvoidFriendlyFireRadius>5</ai_AvoidFriendlyFireRadius>
					<indirectFirePenalty>0.3</indirectFirePenalty>
					<requireLineOfSight>false</requireLineOfSight>
				</verbProps>
				<mainWeaponLabel>direct fire</mainWeaponLabel>
				<secondaryWeaponLabel>indirect fire</secondaryWeaponLabel>
				<description>Extend the sights and switch the launcher to indirect fire mode, or fold it back to direct fire mode.</description>
				<mainCommandIcon>UI/Icons/FireModes/Direct</mainCommandIcon>
				<secondaryCommandIcon>UI/Icons/FireModes/Indirect</secondaryCommandIcon>
				<secondaryWeaponChargeSpeeds>
					<li>30</li>
					<li>50</li>
					<li>70</li>
					<li>90</li>
				</secondaryWeaponChargeSpeeds>
			</li>
		</comps>
	</ThingDef>

