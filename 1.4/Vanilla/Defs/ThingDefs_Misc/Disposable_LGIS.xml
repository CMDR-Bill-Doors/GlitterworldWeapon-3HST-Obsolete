<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!--=====================Disposable LGIS Launchers ===================-->
	<ThingDef Name="BaseDisposableLGIS" Abstract="True">
		<description>Minature missile with low-end laser guidance, designed to be carried by infantry or light armored vehicles, capable of beam-riding guidance, inertia guidance or unguided dumb fire. The missile comes in protective packaging that can also be used as a disposable launcher.</description>
		<soundInteract>Interact_Rifle</soundInteract>
		<category>Item</category>
		<thingClass>ThingWithComps</thingClass>
		<equipmentType>Primary</equipmentType>
		<pathCost>10</pathCost>
		<useHitPoints>True</useHitPoints>
		<selectable>True</selectable>
		<stackLimit>10</stackLimit>
		<drawGUIOverlay>true</drawGUIOverlay>
		<storedConceptLearnOpportunity>BDGW_LGIS</storedConceptLearnOpportunity>
		<resourceReadoutPriority>First</resourceReadoutPriority>

		<graphicData>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>

		<statBases>
			<AccuracyTouch>0.8</AccuracyTouch>
			<AccuracyShort>0.8</AccuracyShort>
			<AccuracyMedium>0.8</AccuracyMedium>
			<AccuracyLong>0.8</AccuracyLong>
			<Mass>2.5</Mass>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<SellPriceFactor>0.5</SellPriceFactor>
			<RangedWeapon_Cooldown>2.85</RangedWeapon_Cooldown>
		</statBases>

		<altitudeLayer>Item</altitudeLayer>
		<alwaysHaulable>True</alwaysHaulable>
		<tickerType>Normal</tickerType>
		<techLevel>Spacer</techLevel>

		<thingCategories>
			<li>Gun3HSTDisposable</li>
		</thingCategories>
		<thingSetMakerTags>
			<li>SingleUseWeapon</li>
			<li>RewardStandardMidFreq</li>
		</thingSetMakerTags>
		<weaponClasses>
			<li>RangedHeavy</li>
			<li>Ranged</li>
		</weaponClasses>

		<weaponTags>
			<li>CE_AI_AOE</li>
			<li>3HST_Weapons</li>
		</weaponTags>

		<generateCommonality>0.6</generateCommonality>
		<comps>
			<li Class="CompProperties_Forbiddable" />
			<li>
				<compClass>CompEquippable</compClass>
			</li>
			<li Class="BillDoorsFramework.CompProperties_ShowConcept">
				<concepts>
					<li>BDGW_LGIS</li>
				</concepts>
			</li>
		</comps>

		<tools>
			<li>
				<label>stock</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
			</li>
			<li>
				<label>barrel</label>
				<capacities>
					<li>Blunt</li>
					<li>Poke</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
			</li>
		</tools>

		<modExtensions>
			<li Class="BillDoorsFramework.ModExtension_VerbNotUnderRoof">
				<appliesInPrimaryMode>false</appliesInPrimaryMode>
			</li>
		</modExtensions>
	</ThingDef>

	<!--===================Direct launchers/ammo=============-->
	<ThingDef ParentName="BaseDisposableLGIS">
		<defName>Gun_3HST_LGIS_thermobaric</defName>
		<label>thermobaric LGIS tube</label>
		<projectileWhenLoaded>Projectile_70mmLGIS_Thermobaric_Indirect</projectileWhenLoaded>
		<generateCommonality>0.5</generateCommonality>
		<graphicData>
			<texPath>Things/Weapons/Disposable_LGIS/thermobaric</texPath>
		</graphicData>
		<weaponTags>
			<li>GunHeavy</li>
			<li>3HST_LGIS_Disposable_Adv</li>
			<li>GunSingleUse</li>
		</weaponTags>
		<verbs>
			<li>
				<verbClass>BillDoorsFramework.Verb_ShootNotUnderRoofOneUse</verbClass>
				<defaultProjectile>Projectile_70mmLGIS_Thermobaric</defaultProjectile>
				<hasStandardCommand>true</hasStandardCommand>
				<ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
				<warmupTime>3.4</warmupTime>
				<burstShotCount />
				<range>45</range>
				<minRange>10</minRange>
				<forcedMissRadius>3</forcedMissRadius>
				<targetParams>
					<canTargetLocations>true</canTargetLocations>
				</targetParams>
				<soundCast>InfernoCannon_Fire</soundCast>
				<soundCastTail>GunTail_Medium</soundCastTail>
				<muzzleFlashScale>5</muzzleFlashScale>
				<ai_AvoidFriendlyFireRadius>5</ai_AvoidFriendlyFireRadius>
			</li>
		</verbs>
		<comps>
			<li Class="BillDoorsFramework.CompProperties_SecondaryVerb">
				<verbProps>
					<verbClass>BillDoorsFramework.Verb_ShootNotUnderRoofOneUse</verbClass>
					<defaultProjectile>Projectile_70mmLGIS_Thermobaric_Indirect</defaultProjectile>
					<hasStandardCommand>true</hasStandardCommand>
					<forcedMissRadius>7</forcedMissRadius>
					<warmupTime>6.4</warmupTime>
					<range>60</range>
					<minRange>20</minRange>
					<targetParams>
						<canTargetLocations>true</canTargetLocations>
					</targetParams>
					<soundCast>InfernoCannon_Fire</soundCast>
					<muzzleFlashScale>5</muzzleFlashScale>
					<ai_AvoidFriendlyFireRadius>5</ai_AvoidFriendlyFireRadius>
					<requireLineOfSight>false</requireLineOfSight>
				</verbProps>
				<mainWeaponLabel>direct fire</mainWeaponLabel>
				<secondaryWeaponLabel>indirect fire</secondaryWeaponLabel>
				<description>Extend the sights and switch the launcher to indirect fire mode, or fold it back to direct fire mode.</description>
				<mainCommandIcon>UI/Icons/FireModes/Direct</mainCommandIcon>
				<secondaryCommandIcon>UI/Icons/FireModes/Indirect</secondaryCommandIcon>
			</li>
		</comps>
		<modExtensions>
			<li Class="BillDoorsFramework.ModExtension_LGISdualModeNPCpatch">
				<chance>0.5</chance>
			</li>
			<li Class="BillDoorsFramework.ModExtension_VerbNotUnderRoof">
				<appliesInPrimaryMode>false</appliesInPrimaryMode>
			</li>
		</modExtensions>
	</ThingDef>

